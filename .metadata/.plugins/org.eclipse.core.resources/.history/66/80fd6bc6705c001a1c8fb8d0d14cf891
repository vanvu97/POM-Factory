package Test;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;
import java.io.IOException;
import java.io.Reader;

public class Test_Read_txt {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		
		String dir = System.getProperty("user.dir") + File.separator +"\\src\\data\\" + File.separator + "RecipeLib.xlsx";
		FileReader fr = new FileReader(dir);
        BufferedReader br = new BufferedReader(dir);
        String s="";
        s=br.readLine();
        System.out.println(s);
        
        
        for(int i = 1; i<=Integer.parseInt(s) ; i++)
        {
        	System.out.println("Phan tu i " + i);
        	// dung code tao recipe o day
        }
	}

}
